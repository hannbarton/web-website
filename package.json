{
  "name": "boiler",
  "version": "1.0.0",
  "description": "starting boilerplate",
  "main": "index.js",
  "scripts": {
    "seed": "node script/seed.js",
    "start": "nodemon --exec babel-node server/index.js",
    "build-client": "webpack",
    "build-client-watch": "webpack",
    "deploy": "script/deploy",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' webpack-dev-server",
    "start-server": "nodemon server/index.js -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register"
  },
  "keywords": [],
  "author": "Hannah Barton",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.0.7",
    "nodemon": "^1.18.10",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "@babel/preset-env": "^7.3.1",
    "babel": "^6.23.0",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "styled-components": "^4.1.3"
  }
}
